<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#56888991"/>
            <corners android:bottomLeftRadius="8dp"
                android:bottomRightRadius="6.87dp"
                android:topLeftRadius="6.87dp" android:topRightRadius="6.87dp"/>
        </shape>
    </item>
    <item>
        <clip>
            <shape android:shape="rectangle">
                <gradient android:angle="270.0"
                    android:endColor="#ba5d72eb"
                    android:startColor="#5e72eb" android:type="linear"/>
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
